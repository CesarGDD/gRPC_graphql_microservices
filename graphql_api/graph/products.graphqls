type Product {
  productId: Int!
  userId: Int!
  name: String!
  url: String!
  price: Int!
  description: String!
  title: String!
  userOwner: User!
}


type ProductResponse {
  product: Product!
}

input NewProductInput {
  name: String!
  userId: Int!
  url: String!
  price: Int!
  description: String!
  title: String!
}

input UpdateProductInput {
  productId: Int!
  name: String!
  url: String!
  price: Int!
  description: String!
  title: String!
}


