enum Role {
  ADMIN
  CUSTOMER
}

type User {
  userId: Int!
  username: String!
  role: Role!
  products: [Product!]
  orders: [OrderDetails!]
}

type UserResponse {
  userId: Int!
  username: String!
  role: Role!
}

input RegisterUserInput {
  username: String!
  password: String!
  role: Role!
}

input UpdateUserInput {
  userId: Int!
  username: String!
  password: String!
  role: Role!
}


